<configure
    xmlns="http://namespaces.zope.org/genericsetup"
    i18n_domain="jazkarta.shop">

  <registerProfile
      name="default"
      title="jazkarta.shop"
      directory="profiles/default"
      description="A flexible web-based shop."
      provides="Products.GenericSetup.interfaces.EXTENSION"
      />

  <upgradeDepends
      profile="jazkarta.shop:default"
      source="1"
      destination="2"
      title="Install browser layer"
      description=""
      import_steps="browserlayer"
      />

  <upgradeDepends
      profile="jazkarta.shop:default"
      source="2"
      destination="3"
      title="Add Product content type"
      description=""
      import_steps="typeinfo"
      />

  <upgradeDepends
      profile="jazkarta.shop:default"
      source="3"
      destination="4"
      title="Add Coupon content type"
      description=""
      import_steps="typeinfo"
      />

  <upgradeDepends
      profile="jazkarta.shop:default"
      source="4"
      destination="5"
      title="Add tax handlers setting"
      description=""
      import_steps="plone.app.registry"
      />

  <upgradeDepends
      profile="jazkarta.shop:default"
      source="5"
      destination="6"
      title="Add taxjar api key to shop settings"
      description=""
      import_steps="plone.app.registry"
      />

  <upgradeDepends
      profile="jazkarta.shop:default"
      source="6"
      destination="7"
      title="Add order controlpanel"
      description=""
      import_steps="controlpanel"
      />

  <upgradeDepends
      profile="jazkarta.shop:default"
      source="7"
      destination="8"
      title="Add authorize.net keys to shop settings"
      description=""
      import_steps="plone.app.registry"
      />

</configure>
